BASEDIR=3rdparty/colomoto

include Makefile.common
-include build/Makefile.inc

all:

clean:

install:
	install -d $(DESTDIR)$(PINT_SHARE_PATH)/3rdparty
	install -d $(DESTDIR)$(PINT_SHARE_PATH)/$(BASEDIR)
	install -m 0644 $(BASEDIR)/LogicalModel*.jar $(DESTDIR)$(PINT_SHARE_PATH)/$(BASEDIR)
	install -d $(DESTDIR)$(PINT_SHARE_PATH)/bin
	install -m 0755 bin/logicalmodel $(DESTDIR)$(PINT_SHARE_PATH)/bin

# vi:set syntax=make:
